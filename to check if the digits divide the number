int count = 0;
      
      int c=n;
      for(int i =0;i<6;i++){
          int d = n % 10;
          if(d == 0){
              n =n / 10;
             continue;
          }else if(d!=0 && c%d==0){
              
              count=count + 1;
          }
          
          n=n/10;
      }
 return count;
}
